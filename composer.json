{
	"name": "webadmin87/rzwebsys7",
	"description": "RzWebSys7 - CMS powered by Yii Framework 2",
	"keywords": ["rzwebsys7", "yii", "yii2", "cms"],
	"homepage": "http://webadmin87.github.io/rzwebsys7/",
	"type": "project",
	"license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Churkin Anton",
            "email": "webadmin87@gmail.com"
        },
        {
            "name": "Denis Chernyavsky",
            "email": "panopticum87@gmail.com"
        }
    ],
	"support": {
		"issues": "https://github.com/webadmin87/rzwebsys7/issues",
		"source": "https://github.com/webadmin87/rzwebsys7"
	},
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "2.0.5",
		"yiisoft/yii2-bootstrap": "2.0.4",
		"yiisoft/yii2-swiftmailer": "2.0.4",
        "yiisoft/yii2-imagine": "2.0.3",
        "yiisoft/yii2-jui": "2.0.4",
        "mihaildev/yii2-ckeditor": "*",
        "mihaildev/yii2-elfinder": "*",
        "creocoder/yii2-nested-sets": "0.9.*",
		"marciocamello/yii2-x-editable": "dev-master",
		"newerton/yii2-fancybox": "dev-master",
		"2amigos/yii2-multi-select-widget": "*",
		"yiisoft/yii2-sphinx": "2.0.4",
		"bower-asset/angular": "1.3.x",
		"bower-asset/angular-i18n": "1.3.x",
        "vova07/yii2-select2-widget": "0.1.*",
        "zelenin/yii2-slug-behavior": "0.5.*"
	},
	"require-dev": {
		"yiisoft/yii2-codeception": "2.0.4",
		"yiisoft/yii2-debug": "2.0.4",
		"yiisoft/yii2-gii": "2.0.4",
		"yiisoft/yii2-faker": "2.0.3",
        "yiisoft/yii2-apidoc": "2.0.4"
	},
	"config": {
		"process-timeout": 1800
	},
	"extra": {
	  "asset-installer-paths": {
		"npm-asset-library": "vendor/npm",
		"bower-asset-library": "vendor/bower"
	  }
	},
    "autoload": {
        "psr-4": {
            "common\\": "common/",
            "console\\": "console/",
            "app\\": "app/"
        }
    }
}
