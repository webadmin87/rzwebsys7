{
	"name": "webadmin87/rzwebsys7",
	"description": "RzWebSys7 - CMS powered by Yii Framework 2",
	"keywords": ["rzwebsys7", "yii2", "cms"],
	"homepage": "https://github.com/webadmin87/rzwebsys7",
	"type": "project",
	"license": "BSD-3-Clause",
	"support": {
		"issues": "https://github.com/webadmin87/rzwebsys7/issues",
		"source": "https://github.com/webadmin87/rzwebsys7"
	},
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "2.0.3",
		"yiisoft/yii2-bootstrap": "2.0.3",
		"yiisoft/yii2-swiftmailer": "2.0.3",
        "yiisoft/yii2-imagine": "2.0.3",
        "yiisoft/yii2-jui": "2.0.3",
        "mihaildev/yii2-ckeditor": "*",
        "mihaildev/yii2-elfinder": "*",
        "creocoder/yii2-nested-sets": "0.9.*",
